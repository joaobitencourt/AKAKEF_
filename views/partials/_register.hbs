{{#each errors}}
    <div class="alert error-msg normalCase">{{texto}}</div>
    {{else}}
{{/each}}
<span class="titleFinesse">Cadastro</span>
<form class="flexCenterColumn" action="/users/register" method="post">
        <div class="wrapper">
            <div class="input-Data">
                <input type="text" name="nameCli" required>
                <section class="line"></section>
                <label class="title3">Nome</label>
            </div>
            <div class="input-Data">
                <input  type="text" name="emailCli" required>
                <section class="line"></section>
                <label class="title3">Email</label>
            </div> 
             <div class="input-Data">
                <input type="text" name="cellCli" required>
                <section class="line"></section>
                <label class="title3">Numero de Celular</label>
            </div>
            <div class="input-Data">
                <input type="password" name="passCli" required>
                <section class="line"></section>
                <label class="title3">Senha</label>
            </div>
             <div class="input-Data">
                <input type="password" name="passCliConfirm" required>
                <section class="line"></section>
                <label class="title3">Confirmar Senha</label>
            </div>
        </div>
    <input class="linkButton save" type="submit" value="Cadastrar">
</form>